            android:textSize="@dimen/palphone_keypad_numbers_textSize"
            app:layout_constraintEnd_toEndOf="@id/zero_layout"
            app:layout_constraintStart_toStartOf="@id/zero_layout"
            app:layout_constraintTop_toTopOf="@id/zero_layout"
            tools:ignore="SpUsage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:fontFamily="?attr/palphone_keypad_font"
            android:text="@string/pad_star"
            android:textColor="?attr/palphone_keypad_letters_text_color"
            android:textSize="@dimen/palphone_plus_sign_size"
            app:layout_constraintEnd_toEndOf="@id/zero_tv"
            app:layout_constraintStart_toStartOf="@id/zero_tv"
            app:layout_constraintTop_toTopOf="@id/zero_tv"
            tools:ignore="SpUsage" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/hashtag_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/zero_layout"
        app:layout_constraintTop_toBottomOf="@id/nine_layout">

        <TextView
            android:id="@+id/hashtag_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="?attr/palphone_keypad_font"
            android:includeFontPadding="false"
            android:text="@string/pad_sharp"
            android:textColor="?attr/palphone_keypad_number_color"
            android:textSize="@dimen/palphone_keypad_numbers_textSize"
            app:layout_constraintEnd_toEndOf="@id/hashtag_layout"
            app:layout_constraintStart_toStartOf="@id/hashtag_layout"
            app:layout_constraintTop_toTopOf="@id/hashtag_layout"
            tools:ignore="SpUsage" />

